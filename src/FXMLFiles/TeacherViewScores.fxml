<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #fff6cf;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TeacherViewScores">
   <children>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label layoutX="248.0" layoutY="34.0" text="View Scores">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="btnViewScores" layoutX="16.0" layoutY="307.0" mnemonicParsing="false" onAction="#viewScores" text="View Scores" />
      <TableView fx:id="quizTable" layoutX="14.0" layoutY="98.0" prefHeight="200.0" prefWidth="236.0">
        <columns>
          <TableColumn fx:id="quizIDCol" prefWidth="48.0" resizable="false" sortable="false" text="quizID" />
          <TableColumn fx:id="titleCol" prefWidth="187.0" resizable="false" sortable="false" text="Title" />
        </columns>
      </TableView>
      <TableView fx:id="scoreTable" layoutX="274.0" layoutY="98.0" prefHeight="200.0" prefWidth="313.0">
        <columns>
          <TableColumn fx:id="studentCol" prefWidth="238.0" sortable="false" text="Student" />
          <TableColumn fx:id="scoreCol" prefWidth="74.0" sortable="false" text="Score" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
